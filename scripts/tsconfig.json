{
	"compilerOptions": {
		"target": "es2022",
		"module": "es2022",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"moduleResolution": "node",
		"newLine": "crlf",
		"resolveJsonModule": true,
	}
}
