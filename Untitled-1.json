[
  {
    "type": "derived",
    "formula": "2*(didStartingZone + autoAmp) + 5*autoSpeaker",
    "label": "Total Auto Points",
    "id": "totalAutoPoints"
  },
  {
    "type": "derived",
    "formula": "2*teleopSpeaker + teleopAmpSpeaker*5 + teleopAmp",
    "label": "Total Teleop Points",
    "id": "totalTeleopPoints"
  },
  {
    "type": "derived",
    "formula": "5*teleopTrap + endgameSpotlit + multiselect(endgameStage, 'not parked', 0, 'parked (at least partially in the stage zone)', 1, 'on stage (fully off the Ground)', 3)",
    "label": "Total Endgame Points",
    "id": "totalEndgamePoints"
  },
  {
    "type": "derived",
    "formula": "totalAutoPoints + totalTeleopPoints + totalEndgamePoints",
    "label": "Total contributed points",
    "id": "contributedPoints"
  },
  {
    "type": "derived",
    "formula": "autoAmp + autoSpeaker",
    "label": "Total Auto Notes",
    "id": "totalAutoNotes"
  },
  {
    "type": "derived",
    "formula": "teleopAmp + teleopSpeaker + teleopAmpSpeaker",
    "label": "Total Teleop Notes",
    "id": "totalTeleopNotes"
  },
  {
    "type": "derived",
    "formula": "autoAmp + autoSpeaker + teleopAmp + teleopSpeaker + teleopAmpSpeaker",
    "label": "Total Notes",
    "id": "totalNotes"
  },
  {
    "type": "header",
    "label": "Autonomous",
    "id": "lblAuto"
  },
  {
    "type": "checkbox",
    "label": "Did they entirely leave Starting Zone?",
    "id": "didStartingZone"
  },
  {
    "type": "subheader",
    "label": "Auto Speaker",
    "id": "lblSpeakerAuto"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes scored in Speaker",
    "id": "autoSpeaker"
  },
  {
    "type": "counter",
    "variant": "bad",
    "label": "Speaker attempts missed",
    "id": "autoSpeakerMiss"
  },
  {
    "type": "subheader",
    "label": "Auto Amp",
    "id": "lblAmpAuto"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes scored in Amp",
    "id": "autoAmp"
  },
  {
    "type": "counter",
    "variant": "bad",
    "label": "Amp attempts missed",
    "id": "autoAmpMiss"
  },
  {
    "type": "derived",
    "formula": "14.646447 - log(cycleTime, 1.4142136)",
    "label": "Cycle Speed Factor",
    "id": "cycleSpeedFactor"
  },
  {
    "type": "derived",
    "formula": "autoAmp + teleopAmp",
    "label": "Total Amp Notes",
    "id": "totalAmpNotes"
  },
  {
    "type": "derived",
    "formula": "autoSpeaker + teleopSpeaker",
    "label": "Total Speaker Notes",
    "id": "totalSpeakerNotes"
  },
  {
    "type": "derived",
    "formula": "14.646447 - log(max(5, abs(onStageTimeEnd - onStageTimeStart)), 1.4142136)",
    "label": "On Stage Speed Factor",
    "id": "onStageSpeedFactor"
  },
  {
    "type": "derived",
    "formula": "((135 - max(onStageTimeStart, onStageTimeEnd)) / (totalTeleopNotes + teleopTrap)) || 160",
    "label": "Cycle Time",
    "id": "cycleTime"
  },
  {
    "type": "spacer"
  },
  {
    "type": "header",
    "label": "Teleop",
    "id": "lblTeleop"
  },
  {
    "type": "subheader",
    "label": "Teleop Speaker",
    "id": "lblTeleopSpeaker"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes scored in Speaker",
    "id": "teleopSpeaker"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes scored in AMP'ed Speaker",
    "id": "teleopAmpSpeaker"
  },
  {
    "type": "counter",
    "variant": "bad",
    "label": "Speaker attemps missed",
    "id": "teleopSpeakerMiss"
  },
  {
    "type": "subheader",
    "label": "Teleop Amp",
    "id": "lblTeleopAmp"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes scored in Amp",
    "id": "teleopAmp"
  },
  {
    "type": "counter",
    "variant": "bad",
    "label": "Amp attempts missed",
    "id": "teleopAmpMiss"
  },
  {
    "type": "subheader",
    "label": "Passing",
    "id": "lblTeleopPass"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes passed into Wing",
    "id": "teleopPass"
  },
  {
    "type": "subheader",
    "label": "Trap",
    "id": "lblTrapScoring"
  },
  {
    "type": "counter",
    "variant": "standard",
    "label": "Notes scored in Traps (1/Trap)",
    "id": "teleopTrap"
  },
  {
    "type": "spacer"
  },
  {
    "type": "header",
    "label": "Endgame",
    "id": "lblEndgame"
  },
  {
    "type": "slider",
    "variant": "time",
    "label": "How much time was on the clock when they started climbing?",
    "id": "onStageTimeStart",
    "options": {
      "min": 0,
      "max": 90,
      "step": -5
    }
  },
  {
    "type": "slider",
    "variant": "time",
    "label": "How much time was on the clock when they stopped climbing?",
    "id": "onStageTimeEnd",
    "options": {
      "min": 0,
      "max": 90,
      "step": -5
    }
  },
  {
    "type": "multiselect",
    "label": "Stage status of robot at end of match:",
    "options": [
      "Not Parked",
      "Parked (At least partially in the Stage zone)",
      "On Stage (Fully off the ground)"
    ],
    "id": "endgameStage"
  },
  {
    "type": "checkbox",
    "label": "Did they Harmonize (2 or more robots climbed on 1 chain)",
    "id": "endgameHarmonize"
  },
  {
    "type": "checkbox",
    "label": "Robot Spotlit (High Note on same stage as Robot)",
    "id": "endgameSpotlit"
  },
  {
    "type": "spacer"
  },
  {
    "type": "header",
    "label": "General",
    "id": "lblGeneral"
  },
  {
    "type": "checkbox",
    "label": "Defended (stopped or delayed at least one score)?",
    "id": "defended"
  },
  {
    "type": "checkbox",
    "label": "Died during the match (or never started)?",
    "id": "diedDuringMatch"
  },
  {
    "type": "checkbox",
    "label": "Recovered from freeze?",
    "id": "recoveredFromFreeze"
  },
  {
    "type": "derived",
    "formula": "1 - diedDuringMatch + 0.5*recoveredFromFreeze",
    "label": "Reliability factor",
    "id": "reliabilityFactor"
  },
  {
    "type": "checkbox",
    "label": "Outstanding performance? (If so, definitely explain why!)",
    "id": "outstandingPerformance"
  },
  {
    "type": "textblock",
    "label": "Other comments and notes:",
    "id": "otherNotes"
  }
]