{
  "name": "voyager-sst",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "postinstall": "yarn smui-theme",
    "update": "yarn upgrade --pattern 'scoutradioz*' --ignore-platform",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "setup": "yarn install --ignore-platform",
    "deploy-test": "sst deploy --stage test",
    "deploy-qa": "sst deploy --stage qa",
    "deploy-prod": "sst deploy --stage prod",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev"
  },
  "devDependencies": {
    "@smui-extra/autocomplete": "^7.0.0",
    "@smui-extra/bottom-app-bar": "^7.0.0",
    "@smui/button": "^7.0.0",
    "@smui/card": "^7.0.0",
    "@smui/checkbox": "^7.0.0",
    "@smui/circular-progress": "^7.0.0",
    "@smui/common": "^7.0.0",
    "@smui/data-table": "^7.0.0",
    "@smui/dialog": "^7.0.0",
    "@smui/drawer": "^7.0.0",
    "@smui/fab": "^7.0.0",
    "@smui/form-field": "^7.0.0",
    "@smui/icon-button": "^7.0.0",
    "@smui/layout-grid": "^7.0.0",
    "@smui/linear-progress": "^7.0.0",
    "@smui/list": "^7.0.0",
    "@smui/paper": "^7.0.0",
    "@smui/ripple": "^7.0.0",
    "@smui/select": "^7.0.0",
    "@smui/slider": "^7.0.0",
    "@smui/snackbar": "^7.0.0",
    "@smui/switch": "^7.0.0",
    "@smui/tab": "^7.0.0",
    "@smui/tab-bar": "^7.0.0",
    "@smui/textfield": "^7.0.0",
    "@smui/tooltip": "^7.0.0",
    "@smui/top-app-bar": "^7.0.0",
    "@smui/touch-target": "^7.0.0",
    "@sveltejs/kit": "^2.5.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/js-cookie": "^3.0.6",
    "@types/qrcode": "^1.5.5",
    "@types/ua-parser-js": "^0.7.39",
    "arctic": "^1.1.3",
    "autoprefixer": "^10.4.16",
    "constructs": "10.2.69",
    "dotenv": "^16.4.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "smui-theme": "^7.0.0-beta.15",
    "sst": "^2.40.1",
    "svelte": "4.2.10",
    "svelte-check": "^3.6.3",
    "svelte-kit-sst": "^2.40.1",
    "svelte-markdown": "^0.4.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.2",
    "vite": "^5.2.12",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0"
  },
  "type": "module",
  "dependencies": {
    "dexie": "^3.2.4",
    "js-cookie": "^3.0.5",
    "lucia": "^3.0.1",
    "mongodb": "4.13.0",
    "oslo": "^1.2.0",
    "qrcode": "^1.5.3",
    "scoutradioz-helpers": "../scoutradioz-helpers/",
    "scoutradioz-utilities": "../scoutradioz-utilities/",
    "ua-parser-js": "^1.0.37"
  }
}
