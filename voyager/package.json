    "svelte-markdown": "^0.4.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0"
  },
  "type": "module",
  "dependencies": {
    "dexie": "^4.0.10",
    "js-cookie": "^3.0.5",
    "lucia": "^3.2.2",
    "mongodb": "6.12.0",
    "oslo": "^1.2.1",
    "qrcode": "^1.5.4",
    "scoutradioz-helpers": "../scoutradioz-helpers/",
    "scoutradioz-utilities": "../scoutradioz-utilities/",
    "ua-parser-js": "^2.0.0"
  }
